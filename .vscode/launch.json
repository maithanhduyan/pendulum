{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch index.html in Chrome",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceFolder}/index.html",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "runtimeArgs": [
        "--remote-debugging-port=9222"
      ]
    }
  ]
}
